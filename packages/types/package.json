{
  "name": "@feathers-playground/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for Feathers Playground",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "feathers",
    "feathersjs",
    "types",
    "typescript",
    "api",
    "playground"
  ],
  "author": "Feathers Playground Team",
  "license": "MIT",
  "homepage": "https://github.com/feathersjs-ecosystem/feathers-playground#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/feathersjs-ecosystem/feathers-playground.git"
  },
  "bugs": {
