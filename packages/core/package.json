{
  "name": "feathers-playground",
  "version": "1.0.2",
  "description": "Interactive API playground and testing environment for FeathersJS v5 applications - Core integration package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "feathers",
    "feathersjs",
    "api",
    "playground",
    "swagger",
    "postman",
    "testing",
    "development",
    "rest",
    "graphql"
  ],
  "author": "Shahboz Munirov",
  "license": "MIT",
  "homepage": "https://github.com/shakhbozmn/feathers-board#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/shakhbozmn/feathers-board.git"
  },
  "bugs": {
    "url": "https://github.com/shakhbozmn/feathers-board/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts --fix",
    "test": "echo 'No tests yet'",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@feathers-playground/types": "^1.0.1"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "@types/express": "^4.17.21"
  },
  "peerDependencies": {
    "@feathersjs/feathers": "^5.0.0",
    "@feathersjs/express": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  }
